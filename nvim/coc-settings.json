{
    "suggest.noselect": false,
    "suggest.keepCompleteopt": true,
    "suggest.floatEnable": true,
    "suggest.enablePreview": false,
    "languageserver": {
      "golang": {
        "command": "gopls",
        "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
        "filetypes": ["go"],
        "initializationOptions": {
          "usePlaceholders": true
        }
      }
    }
}
